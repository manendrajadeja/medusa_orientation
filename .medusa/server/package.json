{
  "name": "medusa-store",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "start": "medusa start",
    "dev": "medusa develop",
    "job:sync": "npx medusa exec src/jobs/sync-products.ts",
    "job:sync:dry": "SYNC_PRODUCTS_DRY_RUN=true npx medusa exec src/jobs/sync-products.ts",
    "job:sync:dist": "npx medusa exec dist/jobs/sync-products.js",
    "inspect:product": "npx medusa exec src/scripts/inspect-product.ts",
    "create:from-mapped": "npx medusa exec src/scripts/create-from-mapped.ts",
    "missing-products": "npx medusa exec src/scripts/missing-products.ts",
    "link-categories": "npx medusa exec src/scripts/link-categories.ts",
    "run-sync-full": "npx ts-node src/scripts/run-sync-full.ts",
    "list-synced": "npx medusa exec src/scripts/list-synced.ts"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.12.4",
    "@medusajs/cli": "2.12.4",
    "@medusajs/framework": "2.12.4",
    "@medusajs/medusa": "2.12.4"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.12.4",
    "@swc/core": "^1.7.28",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.14",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  }
}